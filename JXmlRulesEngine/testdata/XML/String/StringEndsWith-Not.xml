<test>
<request>
  <ruledata>
    <If Operator="AND" Name="c1" Version="1">
      <Condition Operator="StringEndsWith" Not="true">
        <Value Source="InputModel">
          <Path>data0</Path>
          <Bean Object="context">session.user.fname</Bean>
        </Value>
        <Value Source="InputModel">
          <Path>data1</Path>
          <Bean Object="context">session.user.fname</Bean>
        </Value>
      </Condition>
    </If>
    <Actions Name="t1" Version="1">
      <Action Operator="OutputValueSet">
        <Path>/outdata/compare</Path>
        <Value Source="Constant">
          <Data>yes</Data>
        </Value>
      </Action>
    </Actions>
    <Actions Name="t2" Version="1">
      <Action Operator="OutputValueSet">
        <Path>/outdata/compare</Path>
        <Value Source="Constant">
          <Data>no</Data>
        </Value>
      </Action>
    </Actions>
    <Rule Name="rule1" Version="1">
	 <Case>
      <If Operator="AND" Name="ic1" Version="1">
        <Condition Operator="Include">
          <Path>c1</Path>
        </Condition>
      </If>
      <Then Version="1">
        <Action Operator="Include">
          <Path>t1</Path>
        </Action>
      </Then>
	 </Case>
      <Else Version="1">
        <Action Operator="Include">
          <Path>t2</Path>
        </Action>
      </Else>
    </Rule>
  </ruledata>
  <input>
    <data0>bar this foo and foo is ofo</data0>
    <data1>fo</data1>
  </input>
</request>
<response><output><outdata><compare>no</compare></outdata></output></response>
</test>