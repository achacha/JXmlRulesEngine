<?xml version="1.0" encoding="utf-8"?>
<Rule Version="1">
  <Case>
    <If Operator="AND">
      <Condition Operator="Include">
        <Path>number/NumberEquals.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberGreaterThan.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberGreaterThanOrEquals.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberLessThan.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberLessThanOrEquals.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberBetweenExclusive.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberBetweenInclusive.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberDialExclusive.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberDialInclusive.conditions</Path>
      </Condition>
    </If>
    <Then>
      <Action Operator="Include">
        <Path>Yes.actions</Path>
      </Action>
    </Then>
  </Case>
  <Else>
    <Action Operator="Include">
      <Path>No.actions</Path>
    </Action>
  </Else>
</Rule>
