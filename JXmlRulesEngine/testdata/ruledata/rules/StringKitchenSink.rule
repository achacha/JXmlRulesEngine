<?xml version="1.0" encoding="utf-8"?>
<Rule Version="1">
  <Case>
    <If Operator="AND">
      <Condition Operator="Include">
        <Path>string/StringEquals.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringContains.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringStartsWith.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringEndsWith.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringGreaterThan.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringLessThan.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringRegExp.conditions</Path>
      </Condition>
    </If>
    <Then>
      <Action Operator="Include">
        <Path>Yes.actions</Path>
      </Action>
    </Then>
  </Case>
  <Else>
    <Action Operator="Include">
      <Path>No.actions</Path>
    </Action>
  </Else>
</Rule>
