<?xml version="1.0" encoding="utf-8"?>
<Rule Version="1">
  <Case>
    <If Operator="AND">
      <Condition Operator="Include">
        <Path>date/DateEquals.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>date/DateBefore.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>date/DateAfter.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>date/DateBetween.conditions</Path>
      </Condition>
    </If>
    <Then>
      <Action Operator="Include">
        <Path>Yes.actions</Path>
      </Action>
    </Then>
  </Case>
  <Else>
    <Action Operator="Include">
      <Path>No.actions</Path>
    </Action>
  </Else>
</Rule>
