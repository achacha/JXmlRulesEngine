<?xml version="1.0" encoding="utf-8"?>
<Rule Version="1">
  <Case>
    <If Operator="AND">
      <Condition Operator="Include">
        <Path>boolean/BooleanEquals.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>boolean/BooleanTrue.conditions</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>boolean/BooleanFalse.conditions</Path>
      </Condition>
    </If>
    <Then>
      <Action Operator="Include">
        <Path>Yes.actions</Path>
      </Action>
    </Then>
  </Case>
  <Else>
    <Action Operator="Include">
      <Path>No.actions</Path>
    </Action>
  </Else>
</Rule>
