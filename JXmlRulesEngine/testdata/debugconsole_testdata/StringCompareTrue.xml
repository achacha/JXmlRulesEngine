<?xml version="1.0" encoding="utf-8"?>
<request>
  <rules-to-execute>
    <Rule>rule1.rule</Rule>
  </rules-to-execute>
  <ruledata>
    <If Name="c1.conditions" Version="1" Operator="AND">
      <Condition Operator="StringEquals">
        <Value Source="InputModel">
          <Path>data0</Path>
          <Bean Object="context"><Path>session.user.fname</Path></Bean>
        </Value>
        <Value Source="InputModel">
          <Path>data1</Path>
          <Bean Object="context"><Path>session.user.fname</Path></Bean>
        </Value>
      </Condition>
    </If>
    <Actions Name="yes.actions" Version="1">
      <Action Operator="OutputValueSet">
        <Path>/outdata/compare</Path>
        <Value Source="Constant"><Data>yes</Data></Value>
      </Action>
    </Actions>
    <Actions Name="no.actions" Version="1">
      <Action Operator="OutputValueSet">
        <Path>/outdata/compare</Path>
        <Value Source="Constant"><Data>no</Data></Value>
      </Action>
    </Actions>
    <Rule Name="rule1.rule" Version="1">
      <Case>
        <If Operator="AND">
          <Condition Operator="Include">
            <Path>c1.conditions</Path>
          </Condition>
        </If>
        <Then>
          <Action Operator="Include">
            <Path>yes.actions</Path>
          </Action>
        </Then>
      </Case>
      <Else>
        <Action Operator="Include">
          <Path>no.actions</Path>
        </Action>
      </Else>
    </Rule>
  </ruledata>
  <input>
    <data0>foo1</data0>
    <data1>foo1</data1>
  </input>
</request>