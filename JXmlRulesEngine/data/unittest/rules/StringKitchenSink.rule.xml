<?xml version="1.0" encoding="utf-8"?>
<Rule Version="1">
  <Case>
    <If Operator="AND">
      <Condition Operator="Include">
        <Path>string/StringEquals.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringContains.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringStartsWith.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringEndsWith.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringGreaterThan.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringLessThan.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>string/StringRegExp.conditions.xml</Path>
      </Condition>
    </If>
    <Then>
      <Action Operator="Include">
        <Path>Yes.actions.xml</Path>
      </Action>
    </Then>
  </Case>
  <Else>
    <Action Operator="Include">
      <Path>No.actions.xml</Path>
    </Action>
  </Else>
</Rule>
