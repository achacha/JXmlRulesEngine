<?xml version="1.0" encoding="utf-8"?>
<Rule Version="1">
  <Case>
    <If Operator="AND">
      <Condition Operator="Include">
        <Path>number/NumberEquals.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberGreaterThan.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberGreaterThanOrEquals.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberLessThan.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberLessThanOrEquals.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberBetweenExclusive.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberBetweenInclusive.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberDialExclusive.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>number/NumberDialInclusive.conditions.xml</Path>
      </Condition>
    </If>
    <Then>
      <Action Operator="Include">
        <Path>Yes.actions.xml</Path>
      </Action>
    </Then>
  </Case>
  <Else>
    <Action Operator="Include">
      <Path>No.actions.xml</Path>
    </Action>
  </Else>
</Rule>
