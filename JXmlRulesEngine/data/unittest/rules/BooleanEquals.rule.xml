<?xml version="1.0" encoding="utf-8"?>
<Rule Version="1">
  <Case>
    <If Operator="AND">
      <Condition Operator="Include">
        <Path>boolean/BooleanEquals.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>boolean/BooleanTrue.conditions.xml</Path>
      </Condition>
      <Condition Operator="Include">
        <Path>boolean/BooleanFalse.conditions.xml</Path>
      </Condition>
    </If>
    <Then>
      <Action Operator="Include">
        <Path>Yes.actions.xml</Path>
      </Action>
    </Then>
  </Case>
  <Else>
    <Action Operator="Include">
      <Path>No.actions.xml</Path>
    </Action>
  </Else>
</Rule>
